{
  "name": "electric-playground",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "packageManager": "yarn@4.6.0",
  "engines": {
    "node": ">= 20"
  },
  "scripts": {
    "dev": "vinxi dev",
    "build": "vinxi build",
    "start": "vinxi start",
    "preview": "NODE_ENV=development yarn start",
    "docker.up": "docker compose -f ./docker-compose.yml up -d",
    "docker.down": "docker compose -f ./docker-compose.yml down --remove-orphans",
    "docker.nuke": "yarn docker.down -v",
    "db.generate-migrations": "dotenv -e .env.development -- drizzle-kit generate",
    "db.seed": "dotenv -e .env.development -- bun app/db/seed.ts",
    "db.reset": "dotenv -e .env.development -- bun app/db/reset.ts && yarn db.push",
    "db.push": "dotenv -e .env.development -- drizzle-kit push"
  },
  "dependencies": {
    "@electric-sql/client": "1.0.0-beta.3",
    "@electric-sql/pglite": "0.2.17",
    "@electric-sql/pglite-react": "0.2.17",
    "@electric-sql/pglite-sync": "0.2.20",
    "@electric-sql/react": "1.0.0-beta.4",
    "@tanstack/react-router": "1.104.1",
    "@tanstack/router-devtools": "1.104.3",
    "@tanstack/start": "1.104.2",
    "dayjs": "1.11.13",
    "drizzle-kit": "0.30.4",
    "drizzle-orm": "0.39.3",
    "mobx": "6.13.6",
    "mobx-keystone": "1.11.1",
    "mobx-react-lite": "4.1.0",
    "postgres": "3.4.5",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "remeda": "2.20.1",
    "tailwind-merge": "2.6.0",
    "vinxi": "0.5.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "~7.25.9",
    "@biomejs/biome": "1.9.4",
    "@tailwindcss/vite": "4.0.6",
    "@types/node": "22.13.4",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "dotenv-cli": "~8.0.0",
    "postcss": "8.5.2",
    "tailwindcss": "4.0.0",
    "type-fest": "4.34.1",
    "typescript": "5.7.3",
    "vite": "6.1.0",
    "vite-tsconfig-paths": "5.1.4"
  }
}
