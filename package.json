{
  "name": "electric-playground",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "packageManager": "yarn@4.6.0",
  "engines": {
    "node": ">= 20"
  },
  "scripts": {
    "dev": "vinxi dev",
    "build": "vinxi build",
    "start": "vinxi start",
    "preview": "NODE_ENV=development yarn start",
    "docker.up": "docker compose -f ./docker-compose.yml up -d",
    "docker.down": "docker compose -f ./docker-compose.yml down --remove-orphans",
    "docker.nuke": "yarn docker.down -v",
    "db.generate-migrations": "NODE_ENV=development drizzle-kit generate",
    "db.seed": "NODE_ENV=development tsx app/db/seed.ts",
    "db.reset": "NODE_ENV=development tsx app/db/reset.ts && yarn db.push",
    "db.push": "NODE_ENV=development drizzle-kit push"
  },
  "dependencies": {
    "@electric-sql/client": "1.0.0-beta.3",
    "@electric-sql/pglite": "0.2.17",
    "@electric-sql/pglite-react": "0.2.17",
    "@electric-sql/pglite-sync": "0.2.20",
    "@electric-sql/react": "1.0.0-beta.4",
    "@fortawesome/fontawesome-svg-core": "6.7.2",
    "@fortawesome/free-solid-svg-icons": "6.7.2",
    "@fortawesome/react-fontawesome": "0.2.2",
    "@tanstack/react-router": "1.105.0",
    "@tanstack/router-devtools": "1.105.0",
    "@tanstack/start": "1.105.4",
    "dayjs": "1.11.13",
    "dotenv-flow": "4.1.0",
    "drizzle-kit": "0.30.4",
    "drizzle-orm": "0.39.3",
    "envalid": "8.0.0",
    "mobx": "6.13.6",
    "mobx-keystone": "1.11.1",
    "mobx-react-lite": "4.1.0",
    "postgres": "3.4.5",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "remeda": "2.20.2",
    "tailwind-merge": "3.0.1",
    "vinxi": "0.5.3",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "~7.25.9",
    "@biomejs/biome": "1.9.4",
    "@dpaskhin/unique": "1.0.0",
    "@faker-js/faker": "9.5.0",
    "@tailwindcss/vite": "4.0.7",
    "@types/node": "22.13.4",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "postcss": "8.5.2",
    "tailwindcss": "4.0.7",
    "tsx": "4.19.2",
    "type-fest": "4.35.0",
    "typescript": "5.7.3",
    "vite": "6.1.0",
    "vite-tsconfig-paths": "5.1.4"
  }
}
